                openTransferRoom(product) {
                    this.selectedProduct = product;
                    this.transferData = {
                        quantity: 1,
                        destinationRoom: '',
                        reason: '',
                        setSalesFloorStatus: !product.onSalesFloor // Default to enabling sales if not on sales floor
                    };
                    this.showTransferModal = true;
