                        name: '',
                        description: '',
                        type: '',
                        discountValue: 0,
                        buyQuantity: 1,
                        getQuantity: 1,
                        minPurchase: 0,
                        usageLimit: null,
                        allCategories: true,
                        applicableCategories: [],
                        applicableProducts: [],
                        excludeGLS: true,
